            "program": "${workspaceFolder}/src/app/app1/index.ts",
            "preLaunchTask": "tsc: app1",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Test App2",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/src/app/app2/index.ts",
            "preLaunchTask": "tsc: app2",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
